[32m+[m[32mlocal function change_dir(prompt_bufnr)[m
[32m+[m	[32mlocal entry = action_state.get_selected_entry()[m
[32m+[m	[32mactions.close(prompt_bufnr)[m
[32m+[m	[32mif entry and entry.value then[m
[32m+[m		[32mvim.cmd("cd " .. vim.fn.fnameescape(entry.value))[m
